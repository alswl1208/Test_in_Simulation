<root BTCPP_format="4" main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <ForceSuccess>
        <KeepRunningUntilFailure name="InspectAllPosition">
          <Sequence>
            <Action ID="GetNextCoordinate" name="get_next_coordinate"/>
            <Sequence name="SingleInspectionSequence">
              <Action ID="MoveToCoordinate" name="go_coordinate"/>
              <ForceSuccess>
                <Sequence name="MoveAndInspect">
                  <Action ID="MoveToCoordinate" name="go_coordinate"/>
                  <Action ID="CaptureImage" name="capture_image"/>
                </Sequence>
              </ForceSuccess>
            </Sequence>
          </Sequence>
        </KeepRunningUntilFailure>
      </ForceSuccess>
      <Action ID="MoveToHome" name="move_to_home"/>
    </Sequence>
  </BehaviorTree>
</root>
